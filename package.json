{
    "name": "codeheader",
    "displayName": "CodeHeader",
    "description": "An Useful VSC Extension for inserting header comment automatically and easily",
    "version": "0.0.1",
    "publisher": "wh01am",
    "engines": {
        "vscode": "^1.5.0"
    },
    "author": {
        "name": "wh01am",
        "url": "http://wh01am.com"
    },
    "galleryBanner": {
        "theme": "dark",
        "color": "#1E2327"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wh0197m/CodeHeader.git"
    },
    "icon": "images/icon.png",
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onCommand:extension.insertHeader"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "keybindings": [{
            "command": "extension.insertHeader",
            "key": "ctrl+alt+h",
            "when": "editorTextFocus"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/moment": "^2.13.0",
        "@types/node": "^6.0.40",
        "mocha": "^2.3.3",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    },
    "dependencies": {
        "@types/moment": "^2.13.0",
        "moment": "^2.17.1"
    }
}
